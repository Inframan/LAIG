			</transforms>

			<appearanceref id="inherit" />

			<primitives>
				<rectangle xy1="0 0" xy2="1 1" />
			</primitives>

			<descendants>
			</descendants>
		</node>
		<node id="cubo_bot">
			<transforms>
				<transform type="rotate" axis="x" angle="90" />
			</transforms>

			<appearanceref id="inherit" />

			<primitives>
				<rectangle xy1="0 0" xy2="1 1" />
			</primitives>

			<descendants>
			</descendants>
		</node>

	</graph>
</anf>
